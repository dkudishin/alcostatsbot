<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="inMemoryStorage" class="storage.InMemoryStorage"/>

    <bean id="consoleExport" class="export.ConsoleExport">
        <constructor-arg ref="inMemoryStorage"/>
    </bean>
    <bean id="telegramBotsApi" class="org.telegram.telegrambots.meta.TelegramBotsApi">
    </bean>
    <bean id="alcoStatsBot" class="common.AlcoStatsBot">
        <constructor-arg ref="inMemoryStorage"/>
    </bean>

</beans>